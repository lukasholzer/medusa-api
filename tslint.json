{
  "extends": "tslint-config-standard",
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "space-before-function-paren":false,
    "semicolon": true,
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
